<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="LoginServer" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug - Win32">
				<Option output="../bin/Debug/server/LoginServer" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../bin/Debug/server" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<ExtraCommands>
					<Add after="cmd /c scripts\debug-post.bat" />
				</ExtraCommands>
			</Target>
			<Target title="Release - Win32">
				<Option output="../bin/Release/server/LoginServer" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
				<ExtraCommands>
					<Add after="cmd /c scripts\release-post.bat" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-std=c++11" />
			<Add option="-fexceptions" />
			<Add option="-fpermissive" />
			<Add option="-Wno-misleading-indentation" />
			<Add option="-Wno-unused-but-set-variable" />
			<Add directory="../config" />
			<Add directory="../Game/include" />
			<Add directory="../deps/win32/RakNet/include" />
		</Compiler>
		<Linker>
			<Add library="ws2_32" />
			<Add library="RakNet" />
			<Add directory="../deps/win32/RakNet/lib" />
		</Linker>
		<Unit filename="LoginServer.cpp" />
		<Unit filename="LoginServer.h" />
		<Unit filename="scripts/debug-post.bat">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="scripts/release-post.bat">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
